<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="acquirer_form_swedbankpay" model="ir.ui.view">
            <field name="name">acquirer.form.swedbankpay</field>
            <field name="model">payment.acquirer</field>
            <field name="inherit_id" ref="payment.acquirer_form" />
            <field name="arch" type="xml">
                <xpath expr='//group[@name="acquirer"]' position='inside'>
                    <group attrs="{'invisible': [('provider', '!=', 'swedbankpay')]}">
                        <field name="swedbankpay_merchant_id" />
                        <field name="swedbankpay_account_nr" />
                        <field name="swedbankpay_view" />
                        <!--
                             <a colspan="2" href="https://www.odoo.com/documentation/user/online/ecommerce/shopper_experience/paypal.html" target="_blank">How to configure your paypal account?</a>
                        -->
                    </group>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
